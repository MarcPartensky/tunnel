[Unit]
Description=AutoSSH tunnel to proxmox server
After=network.target

[Service]
# User=marc
Environment="AUTOSSH_GATETIME=0"
ExecStart=autossh -M 0 -o PubkeyAuthentication=yes -o PasswordAuthentication=no -o ServerAliveInterval=30 -o ServerAliveCountMax=3 -NL 8006:192.168.0.16:8006 -i ~/.ssh/id_rsa passepartix

[Install]
WantedBy=multi-user.target
